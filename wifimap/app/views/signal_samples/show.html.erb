<%- model_class = SignalSample -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:signal) %>:</strong></dt>
  <dd><%= @signal_sample.signal %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lat) %>:</strong></dt>
  <dd><%= @signal_sample.lat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lng) %>:</strong></dt>
  <dd><%= @signal_sample.lng %></dd>
  <dt><strong><%= model_class.human_attribute_name(:access_point_id) %>:</strong></dt>
  <dd><%= @signal_sample.access_point_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              access_point_signal_samples_path(@access_point), :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_access_point_signal_sample_path(@access_point, @signal_sample), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              access_point_signal_samples_path(@access_point, @signal_sample),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
